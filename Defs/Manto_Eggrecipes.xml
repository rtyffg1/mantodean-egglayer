<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
      <defName>CreateMantoqueenegg</defName>
      <label>Create Mantodean queen egg</label>
      <description>Create an egg of Mantodean queen/drone</description>
      <jobString>Making Mantodean Spawner egg</jobString>  
      <allowMixingIngredients>false</allowMixingIngredients>
	  <skillRequirements>
		<Crafting>4</Crafting>
      </skillRequirements>
      <ingredients>
		<li>
			<filter>
				<thingDefs>
					<li>InsectJelly</li>
				</thingDefs>
			</filter>
			<count>200</count>
		</li>
	  </ingredients>
      <products>
         <MantFarming_QEggPre>1</MantFarming_QEggPre>
      </products>
      <workAmount>1000</workAmount>
      <workSkill>Crafting</workSkill>
    <effectWorking>Repair</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <workSpeedStat>ButcheryMechanoidSpeed</workSpeedStat>
	<recipeUsers>
		<li>MantoJellyMaker</li>
		<li>ArtSpot</li>
     </recipeUsers>
	</RecipeDef>
	
